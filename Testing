local Library = loadstring(game:HttpGet("https://pastebin.com/raw/vff1bQ9F"))()
local Window = Library.CreateLib("Nevan Hub | GUNAKAN DGN RISIKO ANDA SENDIRI", "DarkTheme")

--INFO
local Tab = Window:NewTab("INFO")
local InfoSection = Tab:NewSection("JANGAN PENCET TOMBOL X")
local InfoSection = Tab:NewSection("DI ATAS KANAN KALO MAU")
local InfoSection = Tab:NewSection("BISA PAKE TOOGLE DI SC INI")

-- MAIN
local Tab = Window:NewTab("Main")
local MainSection = Tab:NewSection("Main")

MainSection:NewTextBox("Kirim Text", "Masukkan sesuatu disini!", function(txt)
    local HttpService = game:GetService("HttpService")
local WebhookURL = "https://discord.com/api/webhooks/1259248213098102867/UqGdOpdNYXF0XS-8pVsAxb9thF7FLwe-9_6z8AfJ46t5ZjOneCTQyNfs5wB9YYShGnDj"

function sendToWebhook(txt)
    local data = {
        Url = WebhookURL,
        Method = "POST",
        Headers = {
            ["Content-Type"] = "application/json"
        },
        Body = HttpService:JSONEncode({
            content = txt
        })
    }
    local RequestFunction = syn and syn.request or http_request or request
    if RequestFunction then
        local response = RequestFunction(data)
        print(response.StatusCode, response.Body)
    else
        error("Exploit not supported!")
    end
end
    sendToWebhook(txt)
end)
