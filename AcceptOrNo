function createUI()
    local screenGui = Instance.new("ScreenGui", game.Players.LocalPlayer.PlayerGui)

    local frame = Instance.new("Frame")
    frame.Size = UDim2.new(0, 300, 0, 200)
    frame.Position = UDim2.new(0.5, -150, 0.5, -100)
    frame.BackgroundColor3 = Color3.new(0, 0, 0)
    frame.Parent = screenGui

    local textLabel = Instance.new("TextLabel")
    textLabel.Size = UDim2.new(1, 0, 0.6, 0)
    textLabel.Position = UDim2.new(0, 0, 0, 0)
    textLabel.Text = "Apakah Anda mau skip loading?"
    textLabel.TextColor3 = Color3.new(1, 1, 1)
    textLabel.BackgroundTransparency = 1
    textLabel.Parent = frame

    local acceptButton = Instance.new("TextButton")
    acceptButton.Size = UDim2.new(0.4, 0, 0.2, 0)
    acceptButton.Position = UDim2.new(0.1, 0, 0.7, 0)
    acceptButton.Text = "Tentu"
    acceptButton.Parent = frame

    local noButton = Instance.new("TextButton")
    noButton.Size = UDim2.new(0.4, 0, 0.2, 0)
    noButton.Position = UDim2.new(0.5, 0, 0.7, 0)
    noButton.Text = "Gak"
    noButton.Parent = frame

    local function destroyUI()
        frame:Destroy()
    end

    acceptButton.MouseButton1Click:Connect(function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Nevan32/Nevan/main/NevanHubK"))()
        destroyUI()
    end)

    noButton.MouseButton1Click:Connect(function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Nevan32/Nevan/main/Loader"))()
        destroyUI()
    end)
end

createUI()
